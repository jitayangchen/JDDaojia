<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.com.findfine.jddaojia">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".JDApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".myinfo.SettingActivity" />
        <activity
            android:name=".myinfo.MyAddressActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".myinfo.CreateAddressActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".shop.ShopDetialActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".shop.GoodsDetialActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".shopcart.ShopCartActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".order.NewOrderActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".order.PayActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".order.OrderDetailActivity"></activity>
    </application>

</manifest>